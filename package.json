{
  "name": "boilerplate-frontend-react",
  "description": "Boilerplate Frontend in React",
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "npm run build && npm run html-min",
    "html-min": "html-minifier --input-dir public --output-dir public --file-ext html --remove-comments --collapse-whitespace",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=10.9.2"
  },
  "dependencies": {
    "@fixhq/react-audio-voice-recorder": "2.2.100",
    "@hookform/resolvers": "4.1.3",
    "axios": "1.8.4",
    "bootstrap": "5.3.5",
    "bootstrap-icons": "1.11.3",
    "buffer": "6.0.3",
    "lodash": "4.17.21",
    "react": "19.1.0",
    "react-bootstrap": "2.10.9",
    "react-confetti": "6.4.0",
    "react-dom": "19.1.0",
    "react-easy-crop": "5.4.1",
    "react-hook-form": "7.55.0",
    "react-images-uploading": "3.1.7",
    "react-responsive": "10.0.1",
    "react-router-bootstrap": "0.26.3",
    "react-router-dom": "7.5.0",
    "react-webcam": "7.2.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.8",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.24.0",
    "@types/lodash": "4.17.16",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@types/react-router-bootstrap": "0.26.6",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.24.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "16.0.0",
    "html-minifier": "4.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vite": "6.2.5",
    "vite-plugin-favicons-inject": "2.2.0"
  }
}
